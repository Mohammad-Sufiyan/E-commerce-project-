    <!-- Breadcrumb Section Begin -->

    <!-- Shopping Cart Section Begin -->
    <section class="checkout-section spad">
        <div class="container">
            <form action="/check-out" method="post" class="checkout-form" role="form">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="checkout-content">
                            <a href="/" class="content-btn">Click Here To Login</a>
                        </div>
                        <h4>Biiling Details</h4>
                        <div class="row">
                            <div class="col-lg-6">
                                <label for="fir">First Name<span>*</span></label>
                                <input type="text" id="fir" name="fir" required>
                            </div>
                            <div class="col-lg-6">
                                <label for="last">Last Name<span>*</span></label>
                                <input type="text" id="last" name="last" required>
                            </div>
                            <div class="col-lg-12">
                                <label for="cun">Country<span>*</span></label>
                                <input type="text" id="country" name="country" required>
                            </div>
                            <div class="col-lg-12">
                                <label for="street">Street Address<span>*</span></label>
                                <input type="text" id="address" class="street-first" name="address" required>
                                <input type="text">
                            </div>
                            <div class="col-lg-12">
                                <label for="zip">Postcode / ZIP (optional)</label>
                                <input type="text" id="pinCode" name="pinCode">
                            </div>
                            <div class="col-lg-12">
                                <label for="town">Town / City<span>*</span></label>
                                <input type="text" id="city" name="city" required>
                            </div>
                            <div class="col-lg-6">
                                <label for="email">Email Address<span>*</span></label>
                                <input type="text" id="email" name="email" required>
                            </div>
                            <div class="col-lg-6">
                                <label for="phone">Phone<span>*</span></label>
                                <input type="text" id="phone" name="phone" required>
                            </div>
                           
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="place-order">
                            <h4>Your Order</h4>
                            <div class="order-total">
                                <ul class="order-table">
                                    <li>Product <span>Total</span></li>
                                    <% if(cartData.length>0) {
                                        var i=1 %>
                                        <input type="hidden" name="cartLength" value="<%= cartData.length %>">

                                   <%    cartData.forEach(function(row){
                                       %>
                                       <input type="hidden" name="orderProName<%= i %>" id="orderProName" value="<%= row.product_name %>">
                                       <input type="hidden" name="orderProTotal<%= i %>" id="orderProTotal<%= i %>">
                                    <li class="fw-normal"><%= row.product_name %> <span>
                                        <script>
                                    document.getElementById('orderProTotal'+'<%= i %>').value=sessionStorage.getItem("proTotal"+'<%= i %>');
                                    document.write( sessionStorage.getItem("proTotal"+'<%= i %>')) ;
                                    </script>
                                    </span></li>
                                    <% i=i+1 %>
                                    <% })} %>
                                    <li class="fw-normal">Subtotal <span><script>document.write( sessionStorage.getItem("subTotal")) </script></span></li>
                                    <li class="total-price">Total <span><script>document.write( sessionStorage.getItem("subTotal")) </script></span></li>
                                </ul>
                                <div class="payment-check">
                                    <div class="pc-item">
                                        <label for="pc-check">
                                            Cash on delivery
                                            <input type="checkbox" id="pc-check" name="paymentMethod">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                  
                                </div>
                                <div class="order-btn">
                                    <button type="submit" class="site-btn place-btn" name="placeOrder" onclick="orderMessage()" value="palceOrder">Place Order</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </section>
    <!-- Shopping Cart Section End -->

    <!-- Partner Logo Section Begin -->
    <div class="partner-logo">
